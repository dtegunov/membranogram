<Window x:Class="Membranogram.Controls.PointGroupImportDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Membranogram.Controls"
        mc:Ignorable="d"
        Title="Import Groups From Session" Height="240" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition Height="40" />
        </Grid.RowDefinitions>
        <ListView Name="ListGroups" SelectionMode="Multiple" VerticalAlignment="Stretch" />
        <Button Name="ButtonImport" Grid.Row="1" Content="Import" Click="ButtonImport_OnClick" />
    </Grid>
</Window>
